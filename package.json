    "multer": "^1.4.4",
    "mysql2": "^2.3.3",
    "swagger-ui-express": "^4.4.0",
    "tsoa": "^4.0.0",
    "uuid": "^8.3.2",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.3",
    "@types/express": "^4.17.13",
    "@types/mocha": "^10.0.0",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
